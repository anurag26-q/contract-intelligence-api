# Django Settings
DEBUG=True
SECRET_KEY=your-secret-key-here-change-in-production
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/contract_intelligence

# Google Gemini API (REQUIRED - Add your API key here!)
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_MODEL=gemini-pro
GEMINI_EMBEDDING_MODEL=models/embedding-001
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_TOKENS=2048

# Qdrant Vector Database
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=contracts

# Redis/Celery
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Audit Engine
AUDIT_MODE=hybrid  # options: rules_only, llm_only, hybrid

# Chunking Configuration
CHUNK_SIZE=800
CHUNK_OVERLAP=100

# Webhook Configuration (Optional)
WEBHOOK_ENABLED=False
WEBHOOK_SECRET=your-webhook-secret-here
